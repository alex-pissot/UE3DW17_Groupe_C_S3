<!DOCTYPE html>
<html>
    <head>
        <title>Les auteurs</title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
    </head>

    <body>
        <h1>All Titles</h1>
        <ul>
        <%
            var list = '';
            for (i = 0; i < auteurs.length; i++) {
                
                var str = auteurs[i].fields.auteur;
                if(str.substr(0, 1)=='N'){
                list += '<li>' + auteurs[i].fields.auteur + '</li>';
                }
            }
        %>
        <%- list %>
        </ul>
    </body>
</html>